<!doctype html>
<html class="no-js" lang="{{ request.locale.iso_code }}">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <meta name="theme-color" content="">
    <link rel="canonical" href="{{ canonical_url }}">
    <link rel="preconnect" href="https://cdn.shopify.com" crossorigin>

    {%- if settings.favicon != blank -%}
      <link rel="icon" type="image/png" href="{{ settings.favicon | image_url: width: 32, height: 32 }}">
    {%- endif -%}

    <title>
      {{ page_title }}
      {%- if current_tags %} &ndash; tagged "{{ current_tags | join: ', ' }}"{% endif -%}
      {%- if current_page != 1 %} &ndash; Page {{ current_page }}{% endif -%}
      {%- unless page_title contains shop.name %} &ndash; {{ shop.name }}{% endunless -%}
    </title>

    {% if page_description %}
      <meta name="description" content="{{ page_description | escape }}">
    {% endif %}

    {% render 'meta-tags' %}

    <script src="{{ 'global.js' | asset_url }}" defer="defer"></script>
    {{ content_for_header }}

    {% style %}

      @font-face {
        font-family: 'NHaasGroteskDSProText';
        src: url("{{ 'NHaasGroteskDSPro-55Rg.woff2' | asset_url }}") format('woff2');
        font-weight: 400;
      }

      @font-face {
        font-family: 'NHaasGroteskDSPro';
        src: url("{{ 'NHaasGroteskDSPro-65Md.woff2' | asset_url }}") format('woff2');
        font-weight: 500;
      }
      

      :root {  
        /* Typography */
          --font-body-family: 'NHaasGroteskDSProText';
          --font-heading-family: 'NHaasGroteskDSPro';

          --unit: 16;

          --font-h1-xl-size-desktop: calc(96 / var(--unit) * 1rem); /* 96px */
          --font-h1-size-desktop: calc(64 / var(--unit) * 1rem); /* 64px */
          --font-h2-size-desktop: calc(48 / var(--unit) * 1rem); /* 48px */
          --font-h3-size-desktop: calc(32 / var(--unit) * 1rem); /* 32px */
          --font-h4-size-desktop: calc(24 / var(--unit) * 1rem); /* 24px */
          --font-h5-size-desktop: calc(20 / var(--unit) * 1rem); /* 20px */
          --font-h6-size-desktop: calc(16 / var(--unit) * 1rem); /* 16px */
          --font-text-size-large-desktop: calc(24 / var(--unit) * 1rem); /* 24px */
          --font-text-size-medium-desktop: calc(20 / var(--unit) * 1rem); /* 20px */
          --font-text-size-regular-desktop: calc(16 / var(--unit) * 1rem); /* 16px */
          --font-text-size-small-desktop: calc(14 / var(--unit) * 1rem); /* 14px */
          --font-text-size-tiny-desktop: calc(12 / var(--unit) * 1rem); /* 12px */

          --font-h1-xl-size-tablet: calc(72 / var(--unit) * 1rem); /* 48px */
          --font-h1-size-tablet: calc(52 / var(--unit) * 1rem); /* 32px */
          --font-h2-size-tablet: calc(40 / var(--unit) * 1rem); /* 24px */
          --font-h3-size-tablet: calc(28 / var(--unit) * 1rem); /* 24px */
          --font-h4-size-tablet: calc(20 / var(--unit) * 1rem); /* 20px */
          --font-h5-size-tablet: calc(18 / var(--unit) * 1rem); /* 20px */
          --font-h6-size-tablet: calc(14 / var(--unit) * 1rem); /* 14px */
          --font-text-size-large-tablet: calc(22 / var(--unit) * 1rem); /* 20px */
          --font-text-size-medium-tablet: calc(20 / var(--unit) * 1rem); /* 20px */
          --font-text-size-regular-tablet: calc(16 / var(--unit) * 1rem); /* 16px */
          --font-text-size-small-tablet: calc(14 / var(--unit) * 1rem); /* 14px */
          --font-text-size-tiny-tablet: calc(12 / var(--unit) * 1rem); /* 12px */

          --font-h1-xl-size-mobile: calc(48 / var(--unit) * 1rem); /* 48px */
          --font-h1-size-mobile: calc(40 / var(--unit) * 1rem); /* 32px */
          --font-h2-size-mobile: calc(32 / var(--unit) * 1rem); /* 24px */
          --font-h3-size-mobile: calc(24 / var(--unit) * 1rem); /* 24px */
          --font-h4-size-mobile: calc(16 / var(--unit) * 1rem); /* 20px */
          --font-h5-size-mobile: calc(14 / var(--unit) * 1rem); /* 20px */
          --font-h6-size-mobile: calc(12 / var(--unit) * 1rem); /* 16px */
          --font-text-size-large-mobile: calc(20 / var(--unit) * 1rem); /* 20px */
          --font-text-size-medium-mobile: calc(20 / var(--unit) * 1rem); /* 20px */
          --font-text-size-regular-mobile: calc(16 / var(--unit) * 1rem); /* 16px */
          --font-text-size-small-mobile: calc(14 / var(--unit) * 1rem); /* 14px */
          --font-text-size-tiny-mobile: calc(12 / var(--unit) * 1rem); /* 12px */

          /* Widths */

          --width-x-small: 20em;
          --width-small: 40em;
          --width-medium: 60em;
          --width-large: 80em;
          --width-x-large: 100em;

        /* Colors */
        --color-black: #101010;
        --color-white: #fff;
        --color-off-white: #F9F7F5;
        --color-magenta: #8B146E;
        --color-magenta-dark: #660F51;
        --color-grey-50: #F5F5F5;
        --color-grey-100: #EEEEEE;
        --color-grey-200: #E0E0E0;
        --color-grey-300: #BDBDBD;
        --color-grey-400: #9E9E9E;
        --color-grey-500: #757575;
        --color-grey-600: #616161;
        --color-grey-700: #424242;
        --color-grey-800: #212121;

        --color-primary: var(--color-black);
        --color-secondary: var(--color-white);
        --color-tertiary: var(--color-off-white);
        --color-accent: var(--color-magenta);
      }


    {% endstyle %}

    {{ 'base.css' | asset_url | stylesheet_tag }}
    {{ 'buttons.css' | asset_url | stylesheet_tag }}
    {{ 'colors.css' | asset_url | stylesheet_tag }}
    {{ 'containers.css' | asset_url | stylesheet_tag }}
    {{ 'custom-media.css' | asset_url | stylesheet_tag }}
    {{ 'general-utilities.css' | asset_url | stylesheet_tag }}
    {{ 'icons.css' | asset_url | stylesheet_tag }}
    {{ 'spacing.css' | asset_url | stylesheet_tag }}
    {{ 'typography.css' | asset_url | stylesheet_tag }}
<!-- 
    {%- unless settings.type_body_font.system? -%}
      <link rel="preload" as="font" href="{{ settings.type_body_font | font_url }}" type="font/woff2" crossorigin>
    {%- endunless -%}
    {%- unless settings.type_header_font.system? -%}
      <link rel="preload" as="font" href="{{ settings.type_header_font | font_url }}" type="font/woff2" crossorigin>
    {%- endunless -%} -->

    {%- if settings.predictive_search_enabled -%}
      <link rel="stylesheet" href="{{ 'component-predictive-search.css' | asset_url }}" media="print" onload="this.media='all'">
    {%- endif -%}

    <script>document.documentElement.className = document.documentElement.className.replace('no-js', 'js');
    if (Shopify.designMode) {
      document.documentElement.classList.add('shopify-design-mode');
    }
    </script>
  </head>

  <body class="gradient">
    <a class="skip-to-content-link button visually-hidden" href="#MainContent">
      {{ "accessibility.skip_to_text" | t }}
    </a>

    {%- if settings.cart_type == 'drawer' -%}
      {%- render 'cart-drawer' -%}
    {%- endif -%}

    {% section 'announcement-bar' %}
    {% section 'header' %}
    <main id="MainContent" class="content-for-layout focus-none" role="main" tabindex="-1">
      {{ content_for_layout }}
    </main>

    {% section 'footer' %}

    <ul hidden>
      <li id="a11y-refresh-page-message">{{ 'accessibility.refresh_page' | t }}</li>
      <li id="a11y-new-window-message">{{ 'accessibility.link_messages.new_window' | t }}</li>
    </ul>

    <script>
      window.shopUrl = '{{ request.origin }}';
      window.routes = {
        cart_add_url: '{{ routes.cart_add_url }}',
        cart_change_url: '{{ routes.cart_change_url }}',
        cart_update_url: '{{ routes.cart_update_url }}',
        cart_url: '{{ routes.cart_url }}',
        predictive_search_url: '{{ routes.predictive_search_url }}'
      };

      window.cartStrings = {
        error: `{{ 'sections.cart.cart_error' | t }}`,
        quantityError: `{{ 'sections.cart.cart_quantity_error_html' | t: quantity: '[quantity]' }}`
      }

      window.variantStrings = {
        addToCart: `{{ 'products.product.add_to_cart' | t }}`,
        soldOut: `{{ 'products.product.sold_out' | t }}`,
        unavailable: `{{ 'products.product.unavailable' | t }}`,
      }

      window.accessibilityStrings = {
        imageAvailable: `{{ 'products.product.media.image_available' | t: index: '[index]' }}`,
        shareSuccess: `{{ 'general.share.success_message' | t }}`,
        pauseSlideshow: `{{ 'sections.slideshow.pause_slideshow' | t }}`,
        playSlideshow: `{{ 'sections.slideshow.play_slideshow' | t }}`,
      }
    </script>

    {%- if settings.predictive_search_enabled -%}
      <script src="{{ 'predictive-search.js' | asset_url }}" defer="defer"></script>
    {%- endif -%}
  </body>
</html>
